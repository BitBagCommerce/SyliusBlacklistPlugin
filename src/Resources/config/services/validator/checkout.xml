<?xml version="1.0" encoding="utf-8" ?>

<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" />
        <service id="bitbag_sylius_blacklist_plugin.validator.constraints.checkout.address_type_validator" class="BitBag\SyliusBlacklistPlugin\Validator\Constraints\Checkout\CheckoutAddressTypeValidator">
            <argument type="service" id="bit_bag.sylius_blacklist_plugin.resolver.suspicious_order" />
            <argument type="service" id="bit_bag.sylius_blacklist_plugin.processor.automatic_blacklisting_rules" />
            <argument type="service" id="bit_bag.sylius_blacklist_plugin.converter.fraud_suspicion_common_model" />
            <tag name="validator.constraint_validator" />
        </service>
    </services>
</container>
