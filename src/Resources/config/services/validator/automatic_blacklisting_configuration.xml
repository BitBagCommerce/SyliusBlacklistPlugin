<?xml version="1.0" encoding="utf-8" ?>

<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" />

        <service id="bitbag_sylius_blacklist_plugin.validator.constraints.automatic_blacklisting_configuration.add_fraud_suspicion_row_by_automatic_blacklisting_configuration"
                 class="BitBag\SyliusBlacklistPlugin\Validator\Constraints\AutomaticBlacklistingConfiguration\AddFraudSuspicionByAutomaticBlacklistingConfigurationValidator">
            <argument type="service" id="bitbag_sylius_blacklist_plugin.repository.blacklisting_rule" />
            <argument type="service" id="sylius.context.channel" />
            <tag name="validator.constraint_validator" />
        </service>
    </services>
</container>
