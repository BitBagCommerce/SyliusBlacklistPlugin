{% if data == 'neutral' %}
    <span class="ui icon label">
        {{ 'bitbag_sylius_blacklist_plugin.ui.neutral'|trans }}
    </span>
{% elseif data == 'blacklisted' %}
    <span class="ui icon label black">
        {{ 'bitbag_sylius_blacklist_plugin.ui.blacklisted'|trans }}
    </span>
{% elseif data == 'whitelisted' %}
    <span class="ui basic icon label">
        {{ 'bitbag_sylius_blacklist_plugin.ui.whitelisted'|trans }}
    </span>
{% endif %}